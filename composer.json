{
    "name": "hexlet/code",
    "bin": [
        "bin/gendiff"
    ],
    "description": "A difference calculator is a program that determines the difference between two data structures. A similar mechanism is used when outputting tests or automatically tracking changes in configuration files.",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        },
        "files": [
            "src/Differ.php",
            "src/Parsers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "Hexlet\\Code\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Sergey Danilov"
        }
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "wp-cli/php-cli-tools": "^0.11.22",
        "symfony/yaml": "^6.0.0",
        "funct/funct": "^1.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.8",
        "phpunit/phpunit": "^9.0"
    }
}
